{
  "name": "@sipgate/integration-bridge",
  "version": "0.12.14",
  "description": "sipgate Integration Bridge Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "github:sipgate/integration-bridge",
  "contributors": [
    {
      "name": "Benjamin Kluck",
      "email": "ben@fug.lu"
    },
    {
      "name": "Felix Gladisch",
      "email": "felix@felixgladisch.de"
    }
  ],
  "bugs": {
    "url": "https://github.com/sipgate/integration-bridge/issues"
  },
  "keywords": [
    "integration",
    "express",
    "framework"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "compile": "tsc",
    "build": "npm test && rimraf dist && npm run compile",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "format": "prettier --write '**/*.ts'",
    "link": "npm link"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.15",
    "@types/jest": "^28.1.8",
    "@types/lru-cache": "7.10.9",
    "@types/node": "18.11.18",
    "@types/redis": "4.0.10",
    "husky": "8.0.2",
    "jest": "^28.1.3",
    "lint-staged": "13.1.0",
    "node-mocks-http": "1.12.1",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "awesome-phonenumber": "^3.2.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "lru-cache": "^7.13.1",
    "redis": "^4.2.0"
  }
}
